@startuml
skinparam actorStyle awesome
actor Пользователь

frame Support { 
    Пользователь -> (Система приёма и обработки сообщений) : отправка запроса в тех. поддержку
    Пользователь <- (Система приёма и обработки сообщений) : получение ответа
    frame Moderators {  
    actor Перепись
    Перепись -> Перепись : обработка запроса по внутренним данным
    Перепись <-> (Система приёма и обработки сообщений)
    actor Почтовик
    Почтовик <-> (Система приёма и обработки сообщений)
    database Архив_блокировок
    Почтовик <-> Архив_блокировок
    }
    frame Admin {
    database База_Данных
    actor Администратор
    Администратор <-> База_Данных : поиск данных в БД
    Администратор <-> (Система приёма и обработки сообщений) : обработка запроса на восстановление данных
    }
}
@enduml
