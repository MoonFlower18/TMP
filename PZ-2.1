@startuml
node Сотрудник
node Товар
Сотрудник -- Товар
node Поступление {
node Накладная
node Запись_в_накладной
Накладная --|> Запись_в_накладной
}
Товар ..> Накладная
node Уход {
node Заказ
node Доставщик
Заказ --o Доставщик
}
Товар ..> Заказ
node Списание {
node Документ_о_списании
node Утилизация
Документ_о_списании --* Утилизация
}
Товар ..> Документ_о_списании
node Место_хранения {
node Склад
node Отделение_склада
Склад --|> Отделение_склада 
}
Товар ..> Склад
@enduml
